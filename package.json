{
  "name": "packfinance",
  "version": "0.0.1",
  "description": "Text-based crypto dashboard",
  "main": "index.js",
  "scripts": {
    "start": "node $NODE_DEBUG_OPTION src/index.js --source=gdax",
    "mock": "node $NODE_DEBUG_OPTION src/index.js --source=mock",
    "test": "jest",
    "unit": "jest unit",
    "int": "jest integration",
    "lint": "tslint tests/*.ts -t verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenpack/packfinance.git"
  },
  "keywords": [
    "crypto",
    "blessed",
    "terminal",
    "typescript",
    "node"
  ],
  "author": "Steven Pack <steven.pack.code@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stevenpack/packfinance/issues"
  },
  "homepage": "https://github.com/stevenpack/packfinance#readme",
  "dependencies": {
    "@types/pubsub-js": "^1.5.18",
    "@types/request": "^2.0.8",
    "@types/request-promise": "^4.1.39",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.8.5",
    "gdax": "github:stevenpack/gdax-node",
    "gdax-trading-toolkit": "^0.2.1",
    "install": "^0.10.2",
    "jest": "^21.2.1",
    "limiter": "^1.1.2",
    "log4js": "^2.3.12",
    "moment": "^2.19.2",
    "npm": "^5.5.1",
    "pubsub-js": "^1.5.7",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "ts-jest": "^21.2.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
